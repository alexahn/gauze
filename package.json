{
	"name": "@ahn/gauze",
	"version": "0.0.10",
	"description": "GraphQL server framework",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/alexahn/gauze.git"
	},
	"author": "Alex Ahn",
	"bugs": {
		"url": "https://github.com/alexahn/gauze/issues"
	},
	"homepage": "https://github.com/alexahn/gauze",
	"keywords": [
		"gauze",
		"graphql",
		"framework",
		"structure",
		"server"
	],
	"engines": {
		"node": "22.18.0"
	},
	"type": "module",
	"main": "src/index.js",
	"bin": "./src/command/gauze.js",
	"scripts": {
		"test": "LOG_LEVEL_MINIMUM=4 node test",
		"gauze": "./src/command/gauze.js",
		"start": "node --watch-path=./src ./src/command/gauze.js application serve",
		"start-graphiql": "/bin/zsh -c \"npm run start-graphiql-database & ; npm run start-graphiql-system & ; npm run start-graphiql-environment & ; wait\"",
		"start-graphiql-database": "npx ruru -SP -p 4001 -e http://localhost:4000/database/graphql",
		"start-graphiql-system": "npx ruru -SP -p 4002 -e http://localhost:4000/system/graphql",
		"start-graphiql-environment": "npx ruru -SP -p 4003 -e http://localhost:4000/environment/graphql",
		"start-ui": "/bin/zsh -c \"npm run watch-ui & ; wait\"",
		"build-ui": "/bin/zsh -c \"npm run build-ui-gauze-v1 & ; npm run build-ui-project & ; wait\"",
		"build-ui-gauze-v1": "node ./src/views/gauze/v1/build.js",
		"build-ui-gauze-v2": "node ./src/views/gauze/v2/build.js",
		"build-ui-project": "node ./src/views/project/build.js",
		"watch-ui": "/bin/zsh -c \"npm run watch-ui-gauze-v1 & ; npm run watch-ui-project & ; wait\"",
		"watch-ui-gauze-v1": "node ./src/views/gauze/v1/watch.js",
		"watch-ui-gauze-v2": "node ./src/views/gauze/v2/watch.js",
		"watch-ui-project": "node ./src/views/project/watch.js",
		"serve-ui": "./node_modules/.bin/http-server ./src/views/gauze/build -c-1 --port 3000"
	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.1",
		"globals": "^15.2.0",
		"http-server": "^14.1.1",
		"prettier": "3.2.5",
		"ruru": "^2.0.0-beta.11"
	},
	"dependencies": {
		"@ahn/sinew": "^0.0.4",
		"@koa/cors": "^5.0.0",
		"@koa/etag": "^5.0.1",
		"@koa/router": "^13.1.1",
		"@koa/send": "^6.0.0",
		"@radix-ui/react-icons": "^1.3.2",
		"better-sqlite3": "^9.5.0",
		"dataloader": "^2.2.3",
		"dotenv": "^16.4.5",
		"esbuild": "^0.25.6",
		"find-config": "^1.0.0",
		"graphql": "^16.8.1",
		"jose": "^5.2.4",
		"knex": "^3.1.0",
		"koa": ">=3.0.1",
		"koa-body": "^6.0.1",
		"koa-compress": "^5.1.1",
		"koa-conditional-get": "^3.0.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-redux": "^9.2.0",
		"redux": "^5.0.1",
		"reselect": "^5.1.0",
		"router5": "^8.0.1",
		"router5-plugin-browser": "^8.0.1",
		"tachyons": "^4.12.0",
		"uuid": "^9.0.1",
		"yargs": "^17.7.2"
	}
}
